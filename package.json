{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-pwa": "http-server -p 8080 -c-1 ../linki-b/public/browser",
    "start-noreload": "ng serve --live-reload=false",
    "start-viewphone": "ng serve --host 0.0.0.0 --disable-host-check",
    "start-hot": "NG_HMR_TEMPLATES=1 ng serve --hmr",
    "start-cold": "ng serve --no-hmr",
    "build": "ng build",
     "build-ssr": "ng build && cp dist/front/browser/index.csr.html dist/front/browser/index.html",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:front": "node dist/front/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animation": "4.0.0-beta.8",
